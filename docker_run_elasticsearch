sudo docker run --name ds1-elasticsearch -d -p 9200:9200 -p 9300:9300 -v "$PWD/esdata":/usr/share/elasticsearch/data \
-v "$PWD/esconfig":/usr/share/elasticsearch/config elasticsearch

sudo docker run --name ds1-kibana -d -p 5601:5601 -e ELASTICSEARCH_URL=http://localhost:9200 kibana

sudo docker run -name ds1-nginx -d -p 8080:8080 -v "$PWD/nginx.conf":/etc/nginx/nginx.conf nginx
